function chkDate(){ //ºÏ≤È ±º‰∏Ò Ω
	if(this.value !== ''){
		var err = false;
		var data = this.value.match(/^(\d{4})\-0?(\d{1,2})\-0?(\d{1,2})$/i);
		if(data){
            current_date = new Date();
			if(data[1] < 1998 || data[1] > current_date.getFullYear()){err = true};
			if(data[2] == 0 || data[2] > 12){err = true};
			if(data[3] == 0 || data[3] > 31){err = true};
			if(data[2] == 2 && data[3] > 28){
				err = true;
				if(data[1]%4==0 && data[3] == 29){err = false};
			};
			if((data[2] == 4 || data[2] == 6 || data[2] == 9 || data[2] == 11) && data[3] > 30){err = true};
		}else{
			err = true;
		}
	}else{
		err = false;
	}
	
	if(err){
		if($("#" + this.id + "_err").length == 0){
			$("#" + this.id).after('<span id="' + this.id + '_err" style="font-size:12px;color:#f00;line-height:20px;display:block;">ƒ˙—°‘Òµƒ»’∆⁄≤ª∫œ¿Ì</span>');
		};
		$("#" + this.id + "_err").css('display','block');
	}else{
		$("#" + this.id + "_err").css('display','none');
	};
};
$("#s_time").bind("blur",chkDate);
$("#e_time").bind("blur",chkDate);
//—È÷§±Ìµ•Ã·Ωª
function chkDateForm(form){
    var s_date_value = form.stime.value;
    var e_date_value = form.etime.value;
	var s_date = s_date_value.match(/^(\d{4})\-0?(\d{1,2})\-0?(\d{1,2})$/i);
	var e_date = e_date_value.match(/^(\d{4})\-0?(\d{1,2})\-0?(\d{1,2})$/i);
	if(!s_date || !e_date){
		alert('ƒ˙—°‘Òµƒ»’∆⁄≤ª∫œ¿Ì');
		return false;
	};
    
    /*—°‘Òµƒ»’∆⁄¥Û”⁄µ±«∞»’∆⁄£¨∏¯≥ˆÃ· æ --dalong3 20120920*/
    var s_date_string = new String();
    var current_date = new Date();
    s_date_year = s_date[1];
    e_date_year = e_date[1];
    min_date_year = 1998;
    if((s_date_year - current_date.getFullYear() > 0) || (min_date_year - s_date_year >0) || (s_date_year - e_date_year >0)) {
		alert('ƒ˙—°‘Òµƒ»’∆⁄≤ª∫œ¿Ì');
		return false;
    }
	s_date = new Date(s_date[1],s_date[2] - 1,s_date[3]);
	e_date = new Date(e_date[1],e_date[2] - 1,e_date[3]);


	if(s_date > e_date){
		alert('ƒ˙—°‘Òµƒ»’∆⁄≤ª∫œ¿Ì');
		return false;
	};
	return true;
};

